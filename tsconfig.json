{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@vben/ts-config/web.json",
  "compilerOptions": {
    "baseUrl": ".",
    "types": ["vite/client", "types/global.d.ts"],
    "paths": {
      "@/*": ["src/*"],
      "#/*": ["types/*"]
